<?import javafx.scene.control.*?>
<?import javafx.scene.layout.BorderPane?>

<BorderPane xmlns:fx="http://javafx.com/fxml"
            stylesheets="@Main.css"
            fx:controller="proj8ZhouRinkerSahChistolini.Controllers.ControllerInitializer">
    <top>
        <MenuBar>
            <fx:include fx:id="fileMenu"
                        source="fileMenu.fxml"/>
            <fx:include fx:id="editMenu"
                        source="editMenu.fxml"/>
            <fx:include fx:id="actionMenu"
                        source="actionMenu.fxml"/>
        </MenuBar>
    </top>
    <left>
        <fx:include fx:id="instrumentPane"
                    source="instrumentPanel.fxml"/>
    </left>
    <center>
        <fx:include fx:id="compositionPanel"
                    source="compositionPanel.fxml"/>
    </center>

</BorderPane>